{
  "name": "cuid2gen",
  "version": "0.1.4",
  "description": "A command-line tool for generating CUID2 identifiers",
  "bin": "lib/index.js",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "build": "tsc --outDir lib",
    "dev": "tsx src/index.ts",
    "start": "node lib/index.js",
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
    "prepare": "npm run build"
  },
  "keywords": [
    "cuid",
    "uuid",
    "cli",
    "id",
    "generator"
  ],
  "author": "@raikusy <ping@raikusy.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raikusy/cuid2gen.git"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
